{{> navbar}}
<br/>

<h1>Listado de productos</h1>
{{#if productsExist}}
    {{#each products}}
        <div>
            {{this.id}}<br/>
            {{this.title}}<br/>
            {{this.price}}<br/>
            <img src="{{this.thumbnail}}" class="product-img"/>
        </div>
    {{/each}}
{{/if}}
<button id="formulario" onclick="window.location.href='http://localhost:8080/form';" >Volver al formulario</button>
<br/>
{{> footer}}
<br/>